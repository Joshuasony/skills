<section id="competence" class="section">
  <!-- Profile heading -->
  <div class="row-fluid">
    <div class="span5">
      <span class="section-title">Kompetenzen</span>
    </div>
  </div>

  <!-- Horizontal ruler -->
  <div class="row-fluid">
    <div class="span12">
      <hr>
    </div>
  </div>
    <div id="competence-content-show" class="row-fluid">
      <div class="span12">
        <ul>
          {{#each competences as |competence|}}
            <li>{{competence.description}}</li>
          {{/each}}
        </ul>
      </div>
    </div>
</section>
